<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="background" prefHeight="576.0" prefWidth="1055.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Restaurant_Billing_System.Order_ListController">
    <stylesheets>
        <URL value="@order_list.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableview" layoutX="320.0" layoutY="71.0" prefHeight="483.0" prefWidth="686.0">
        <columns>
          <TableColumn fx:id="col1" prefWidth="238.0" text="Date" />
          <TableColumn fx:id="col2" prefWidth="244.0" text="Amount" />
            <TableColumn fx:id="col3" prefWidth="246.0" text="Username" />
        </columns>
      </TableView>
      <Button fx:id="yo" layoutX="592.0" layoutY="31.0" mnemonicParsing="false" onAction="#click_yo" text="Retrieve From Database" />
      <VBox fx:id="dashboard" alignment="CENTER" prefHeight="586.0" prefWidth="197.0" style="-fx-background-color: #05071F;">
         <children>
            <Label fx:id="name" lineSpacing="10.0" text="G I V E  I N" textFill="WHITE">
               <font>
                  <Font size="27.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label fx:id="name2" text="R E S T A U R A N T" textAlignment="CENTER" textFill="#f8f8f8">
               <padding>
                  <Insets bottom="50.0" />
               </padding>
            </Label>
            <Button fx:id="homebutton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#click_homebutton" prefHeight="71.0" prefWidth="230.0" style="-fx-background-color: #05071F;" text="Home" textFill="WHITE">
               <font>
                  <Font name="HP Simplified" size="22.0" />
               </font>
            </Button>
            <Button fx:id="monthly_entry" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#click_monthly_entry" prefHeight="71.0" prefWidth="230.0" style="-fx-background-color: #05071F;" text="Monthly Entry" textFill="WHITE">
               <font>
                  <Font name="HP Simplified" size="22.0" />
               </font>
            </Button>
            <Button fx:id="take_order" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#click_takeorder" prefHeight="71.0" prefWidth="185.0" style="-fx-background-color: #05071F;" text="Take Order" textFill="WHITE">
               <font>
                  <Font name="HP Simplified" size="22.0" />
               </font>
            </Button>
            <Button fx:id="exit" alignment="CENTER" mnemonicParsing="false" onAction="#click_exit" prefHeight="73.0" prefWidth="230.0" style="-fx-background-color: #05071F;" text="Exit" textFill="WHITE">
               <font>
                  <Font name="HP Simplified" size="22.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
    
</AnchorPane>
